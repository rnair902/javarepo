{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "sites_hxahpi_name": {
            "type": "String"
        },
        "serverfarms_ASP_rsgroup_b3e8_externalid": {
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Web/sites",
            "apiVersion": "2022-03-01",
            "name": "[parameters('sites_hxahpi_name')]",
            "location": "East US",
            "tags": {
                "hidden-link: /app-insights-resource-id": "/subscriptions/de7aad7d-dc4f-46e6-a145-f90fb9a75b79/resourceGroups/rsgroup/providers/Microsoft.Insights/components/hxahpi"
            },
            "kind": "functionapp",
            "properties": {
                "enabled": true,
                "hostNameSslStates": [
                    {
                        "name": "[concat(parameters('sites_hxahpi_name'), '.azurewebsites.net')]",
                        "sslState": "Disabled",
                        "hostType": "Standard"
                    },
                    {
                        "name": "[concat(parameters('sites_hxahpi_name'), '.scm.azurewebsites.net')]",
                        "sslState": "Disabled",
                        "hostType": "Repository"
                    }
                ],
                "serverFarmId": "[parameters('serverfarms_ASP_rsgroup_b3e8_externalid')]",
