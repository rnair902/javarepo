            "type": "Microsoft.Web/sites/hostNameBindings",
            "apiVersion": "2022-03-01",
            "name": "[concat(parameters('sites_hxahpi_name'), '/', parameters('sites_hxahpi_name'), '.azurewebsites.net')]",
            "location": "East US",
            "dependsOn": [
                "[resourceId('Microsoft.Web/sites', parameters('sites_hxahpi_name'))]"
            ],
            "properties": {
                "siteName": "hxahpi",
                "hostNameType": "Verified"
            }
        }
